{
  "manifest_version": 2,

  "name": "Screen Marker",
  "description": "A browser plugin for marking up screen grabs.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Start marking this screen!"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}